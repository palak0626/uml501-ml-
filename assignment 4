      "cell_type": "code",
      "source": [
        "import asyncio\n",
        "from playwright.async_api import async_playwright\n",
        "import pandas as pd"
      ],
      "metadata": {
        "id": "s9MuQhe7BjOz"
      },
      "execution_count": 153,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "async with async_playwright() as p:\n",
        "  browser = await p.chromium.launch(headless=True)\n",
        "  page = await browser.new_page()\n",
        "  await page.goto(\"https://www.imdb.com/chart/top/\")\n",
        "\n",
        "  rows = await page.query_selector_all('tbody.lister-list > tr')\n",
        "\n",
        "  for row in rows:\n",
        "    rank_text = await row.query_selector('td.titleColumn')\n",
        "    rank_text = await rank_text.inner_text()\n",
        "    rank = rank_text.split('.')[0].strip()\n",
        "\n",
        "    title = await (await row.query_selector('td.titleColumn a')).inner_text()\n",
        "    year = await (await row.query_selector('td.titleColumn span')).inner_text()\n",
        "    year = year.strip('()')\n",
        "    rating = await (await row.query_selector('td.ratingColumn.imdbRating strong')).inner_text()\n",
        "\n",
        "    ranks.append(rank)\n",
        "    titles.append(title)\n"
      ],
      "metadata": {
        "id": "o67NFKxEB2Bm"
      },
