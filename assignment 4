    {
      "cell_type": "code",
      "source": [
        "table = soup.find(\"table\", {\"class\": \"zebra fw tb-theme\"})\n",
        "\n",
        "if table is None:\n",
        "    print(\" Could not find the weather table.\")\n",
        "else:\n",
        "    cities, temperatures, conditions = [], [], []\n",
        "\n",
        "    for row in table.find_all(\"tr\")[1:]:  # skip header row\n",
        "        cols = row.find_all(\"td\")\n",
        "        if len(cols) >= 3:\n",
        "          city = cols[0].text.strip()\n",
        "          temp = cols[1].text.strip()\n",
        "          condition = cols[2].text.strip()\n",
        "\n",
        "          cities.append(city)\n",
        "          temperatures.append(temp)\n",
        "          conditions.append(condition)"
      ],
      "metadata": {
        "id": "3sxDesBi_csH"
      },
      "execution_count": 129,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "df = pd.DataFrame({\n",
        "        \"City\": cities,\n",
        "        \"Temperature\": temperatures,\n",
        "        \"Condition\": conditions\n",
        "    })"
      ],
      "metadata": {
        "id": "sPo4Exrj_l7V"
      },
      "execution_count": 130,
      "outputs": []
    },
    {
      "cell_type": "code",
